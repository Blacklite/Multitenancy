{
	"webroot": "wwwroot",
	"version": "1.0.0-*",
	"exclude": [
		"wwwroot"
	],
	"packExclude": [
		"**.kproj",
		"**.user",
		"**.vspscc"
	],
	"commands": {
		"web": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5000"
	},
	"dependencies": {
		"Microsoft.AspNet.Server.IIS": "1.0.0-beta3-*",
		"Microsoft.AspNet.Server.WebListener": "1.0.0-beta3-*",
		"Microsoft.AspNet.Diagnostics": "1.0.0-beta3-*",
		"Microsoft.Framework.Logging.Console": "1.0.0-beta3-*",
		"Blacklite.Framework.Multitenancy": "",
		"Blacklite.Framework.Multitenancy.Autofac": "",
		"Blacklite.Framework.Multitenancy.ApplicationEvents": "",
		"Blacklite.Framework.Multitenancy.Logging": "",
		"Blacklite.Framework.DependencyInjection": "1.0.0-beta-*",
		"Microsoft.AspNet.Mvc": "6.0.0-beta3-*",
		"Newtonsoft.Json": "6.0.6"
	},
	"frameworks": {
		"aspnet50": {
			"frameworkAssemblies": {
				"System.Runtime": "4.0.20.0"
			}
		}
	}
}
